{
  "expo": {
    "name": "1hat-react",
    "slug": "1hat-react",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1c2f7f"
      },
      "permissions": [],
      "config": {
        "usesCleartextTraffic": true
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": []
  }
}
